FILE=fullpath -t $0
OS=qnx
PROCDIR=$YARP_ROOT/bin/$OS

echo "'oly cow..."
echo "be sure you're root..."

if false; then
echo "Starting grabber2 on node 24"
on -n24 slay -Q grabber2
on -d -n24 $PROCDIR/grabber2 -height 128 -width 128 -format bgr -tagged -output /yarp/stereo1/bgr &
sleep 2
fi

echo "Starting grabber on node 14"
on -n14 slay -Q grabber
on -n14 grabber -height 128 -width 128 -format bgr -tagged -output /yarp/stereo1/bgr &
sleep 2

